    "validators": "^0.3.1"
  },
  "devDependencies": {
    "browserify": "^9.0.7",
    "browserify-shim": "^3.8.3",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.4.3",
    "gulp-flatten": "0.0.4",
    "gulp-flatten-requires": "^1.0.0",
    "gulp-header": "^1.2.2",
    "gulp-jshint": "^1.10.0",
    "gulp-plumber": "^1.0.0",
    "gulp-react": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.2",
    "jshint-stylish": "^1.0.0",
    "qqunit": "^0.6.0",
